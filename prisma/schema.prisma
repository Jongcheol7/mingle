generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  id      String   @id  @default(autoincrement())
  clerkId String @unique
  email   String   @unique
  name    String
  createdAt DatetTime @default(now())
}